  "module": "./dist/editor.js",
  "types": "./dist/src/serlo-editor/package/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/editor.js"
    },
    "./dist/editor-tailwind.css": {
      "import": "./dist/editor-tailwind.css",
      "require": "./dist/editor-tailwind.css"
    }
