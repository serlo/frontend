{
  "extends": "@serlo/eslint-config-react",
  "overrides": [
    {
      "files": ["src/pages/**/*"],
      "rules": {
        "import/no-default-export": "off",
        "react/display-name": "off",
        "@typescript-eslint/require-await": "off"
      }
    },
    {
      "files": ["src/data/*/index.ts"],
      "rules": {
        "import/newline-after-import": "off",
        "import/order": "off"
      }
    }
  ]
}
